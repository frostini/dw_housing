<h1>Hello single dwelling</h1>
<p><%=@dwelling.inspect%></p>

<p><%=@dwelling.try(:address).inspect%></p>
<p><%=@dwelling.floor_plans.first.inspect%></p>
<p><%=@dwelling.amenities.first.inspect%></p>
<p><%=@dwelling.dwelling_details.first.inspect%></p>

<%= image_tag @dwelling.image(:large)%>


<%= form_for @dwelling, url: {action: "new_image"}, html: { multipart: true } do |form| %>
  <%= form.file_field :image %>
  <%= form.submit "Create" %>
<% end %>